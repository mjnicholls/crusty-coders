<div class="container">
    <div class="header">
        <div><img src="/img/comment3.png" width="50px" height="50px"></div>
        <div class="text"><p class="yes">Leave a Comment</p></div>
    </div>

    <form method="POST" action="/postCommentApp">
        <div class="form-row">
          <div class="form-group col-md-4 mb-3">
            <label for="inputEmail4">Name</label>
            <input type="text"
            class="form-control"
            id="name"
            placeholder="name"
            name="name"
            required
            value="<%= typeof name != 'undefined' ? name : '' %>">
          </div>
          <div class="form-group col-md-4 mb-3">
            <label for="inputPassword4">Comment</label>
            <input type="text"
            class="form-control"
            id="comment"
            placeholder="comment"
            name="comment"
            required
            value="<%= typeof comment != 'undefined' ? comment : '' %>">
          </div>
        </div>
         
        <button type="submit" class="btn btn-primary">Do it!</button>
       
   
      </form>
    </div>

    <div class="detailBox">
      <% commentappejs.forEach(function(comment) { %>
      <div class="actionBox">
          <ul class="commentList">
              <li>
                  <div class="commenterImage">
                      <img src="/img/profile.png">
                  </div>
                  <div class="commentText">
                      <h5><%= comment.name %></h5>
                      <p class=""><%= comment.comment%></p> <span class="date sub-text">on <%= comment.date.toDateString()%></span>
  
                  </div>
              </li>
              
               
          </ul>
          

      </div>
      <% }) %>
  </div>
    


